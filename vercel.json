{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    },
    {
      "src": "**",
      "use": "@vercel/nextjs",
      "config": {
        "output": "export"
      }
    }
  ],
  "routes": [
    { "src": "/api-docs", "dest": "/index.js" },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {
      "src": "/swagger-ui.html",
      "dest": "/public/swagger-ui.html"
    },
    {
      "src": "/swagger-ui/(.*)",
      "dest": "/public/swagger-ui/$1"
    }
  ]
}
